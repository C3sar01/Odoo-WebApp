<odoo>
  <data>
          <record model="ir.ui.view" id="compra.purchase_list">
              <field name="name">Compras</field>
              <field name="model">compra.purchase</field>
              <field name="arch" type="xml">
                <tree>
                   <field name="name"/>
                   <field name="duration"/>
                   <field name="cost"/>
                   <field name="date"/>
              </tree>
            </field>
           </record>

           <record model="ir.actions.act_window" id="compra.purchase_action_window">
      <field name="name">Listado de compras</field>
      <field name="res_model">compra.purchase</field>
      <field name="view_mode">tree,form</field>
    </record>


     <record model="ir.actions.server" id="compra.purchase_action_server">
      <field name="name">purchase server</field>
      <field name="model_id" ref="model_compra_purchase"/>
      <field name="state">code</field>
      <field name="code">
        action = {
          "type": "ir.actions.act_window",
          "view_mode": "tree,form",
          "res_model": self._name,
        }
      </field>
    </record>

    <menuitem name="Compras" id="compra.menu_root"/>


    <menuitem name="Purchase" id="compra.menu_purchase" parent="compra.menu_root"
              action="compra.purchase_action_window"/>

    </data>
</odoo>